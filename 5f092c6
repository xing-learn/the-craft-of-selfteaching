    120. "#c.NotebookApp.token = ''\n",
    121. "#c.NotebookApp.open_browser = False\n",
    122. "#c.NotebookApp.notebook_dir = '~/'\n",
    123. "#c.NotebookApp.default_url = '/tree'\n",

